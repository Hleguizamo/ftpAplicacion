<?php

namespace FTP\AdministradorBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * LyMediaAssetTranslation
 */
class LyMediaAssetTranslation
{
    /**
     * @var string
     */
    private $lang;

    /**
     * @var string
     */
    private $title;

    /**
     * @var string
     */
    private $description;

    /**
     * @var \FTP\AdministradorBundle\Entity\LyMediaAsset
     */
    private $id;


    /**
     * Set lang
     *
     * @param string $lang
     * @return LyMediaAssetTranslation
     */
    public function setLang($lang)
    {
        $this->lang = $lang;
    
        return $this;
    }

    /**
     * Get lang
     *
     * @return string 
     */
    public function getLang()
    {
        return $this->lang;
    }

    /**
     * Set title
     *
     * @param string $title
     * @return LyMediaAssetTranslation
     */
    public function setTitle($title)
    {
        $this->title = $title;
    
        return $this;
    }

    /**
     * Get title
     *
     * @return string 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set description
     *
     * @param string $description
     * @return LyMediaAssetTranslation
     */
    public function setDescription($description)
    {
        $this->description = $description;
    
        return $this;
    }

    /**
     * Get description
     *
     * @return string 
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set id
     *
     * @param \FTP\AdministradorBundle\Entity\LyMediaAsset $id
     * @return LyMediaAssetTranslation
     */
    public function setId(\FTP\AdministradorBundle\Entity\LyMediaAsset $id)
    {
        $this->id = $id;
    
        return $this;
    }

    /**
     * Get id
     *
     * @return \FTP\AdministradorBundle\Entity\LyMediaAsset 
     */
    public function getId()
    {
        return $this->id;
    }
}
