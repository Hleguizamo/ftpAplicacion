<?xml version='1.0' encoding='utf-8'?>
<rows>
    <page>{{ pagina }}</page>
    <total>{{ maxPagina }}</total>
    <records>{{ numRegistros }}</records>
    {% for entity in entities %}
    <row id="{{ entity.id }}">
        <cell>{{ entity.id }}</cell>
        <cell><![CDATA[{{ entity.nombre }}]]></cell>
        <cell><![CDATA[{{ entity.usuario  }}]]></cell>
        <cell><![CDATA[{{ entity.email }}]]></cell>
        <cell><![CDATA[{% if entity.seguimiento ==1 %}Si{% else %}No{% endif %} ]]></cell>     
        <cell><![CDATA[{% if entity.ultimoIngreso %} {{ entity.ultimoIngreso |date('Y-m-d H:i:s')  }} {% endif %}]]></cell>
        <cell><![CDATA[{% if entity.estado ==1 %}Activo{% else %}Inactivo{% endif %} ]]></cell>                     
    </row>
    {% endfor %}
</rows>