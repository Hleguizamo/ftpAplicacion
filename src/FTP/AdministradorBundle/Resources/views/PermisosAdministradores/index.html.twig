{% extends '::base.html.twig' %}

{% block body -%}
    <h1>PermisosAdministradores list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idpermisos</th>
                <th>Idadministrador</th>
                <th>Idpermiso</th>
                <th>Tipopermiso</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('permisosadministradores_show', { 'id': entity.idPermisos }) }}">{{ entity.idPermisos }}</a></td>
                <td>{{ entity.idAdministrador }}</td>
                <td>{{ entity.idPermiso }}</td>
                <td>{{ entity.tipoPermiso }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('permisosadministradores_show', { 'id': entity.idPermisos }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('permisosadministradores_edit', { 'id': entity.idPermisos }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('permisosadministradores_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
