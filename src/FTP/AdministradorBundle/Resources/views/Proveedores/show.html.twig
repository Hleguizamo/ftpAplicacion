{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Proveedores</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Proveedor</th>
                <td>{{ entity.proveedor }}</td>
            </tr>
            <tr>
                <th>Codigoproveedor</th>
                <td>{{ entity.codigoProveedor }}</td>
            </tr>
            <tr>
                <th>Nitproveedor</th>
                <td>{{ entity.nitProveedor }}</td>
            </tr>
            <tr>
                <th>Carpetatransferencista</th>
                <td>{{ entity.carpetaTransferencista }}</td>
            </tr>
            <tr>
                <th>Carpetaconvenios</th>
                <td>{{ entity.carpetaConvenios }}</td>
            </tr>
            <tr>
                <th>Estadotransferencia</th>
                <td>{{ entity.estadoTransferencia }}</td>
            </tr>
            <tr>
                <th>Estadoconvenios</th>
                <td>{{ entity.estadoConvenios }}</td>
            </tr>
            <tr>
                <th>Encargadoproveedor</th>
                <td>{{ entity.encargadoProveedor }}</td>
            </tr>
            <tr>
                <th>Emailencargado</th>
                <td>{{ entity.emailEncargado }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Ultimospedidos</th>
                <td>{{ entity.ultimosPedidos|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ entity.cantidad }}</td>
            </tr>
            <tr>
                <th>Ultcarguetransferencista</th>
                <td>{{ entity.ultCargueTransferencista|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Ultcargueconvenios</th>
                <td>{{ entity.ultCargueConvenios|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('proveedores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('proveedores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
