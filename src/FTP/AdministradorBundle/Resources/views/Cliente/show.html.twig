{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Cliente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Zona</th>
                <td>{{ entity.zona }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ entity.codigo }}</td>
            </tr>
            <tr>
                <th>Bloqueod</th>
                <td>{{ entity.bloqueoD }}</td>
            </tr>
            <tr>
                <th>Drogueria</th>
                <td>{{ entity.drogueria }}</td>
            </tr>
            <tr>
                <th>Nit</th>
                <td>{{ entity.nit }}</td>
            </tr>
            <tr>
                <th>Bloqueor</th>
                <td>{{ entity.bloqueoR }}</td>
            </tr>
            <tr>
                <th>Asociado</th>
                <td>{{ entity.asociado }}</td>
            </tr>
            <tr>
                <th>Nitdv</th>
                <td>{{ entity.nitDv }}</td>
            </tr>
            <tr>
                <th>Direcion</th>
                <td>{{ entity.direcion }}</td>
            </tr>
            <tr>
                <th>Codmun</th>
                <td>{{ entity.codMun }}</td>
            </tr>
            <tr>
                <th>Ciudad</th>
                <td>{{ entity.ciudad }}</td>
            </tr>
            <tr>
                <th>Ruta</th>
                <td>{{ entity.ruta }}</td>
            </tr>
            <tr>
                <th>Ungeogra</th>
                <td>{{ entity.unGeogra }}</td>
            </tr>
            <tr>
                <th>Depto</th>
                <td>{{ entity.depto }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th>Centro</th>
                <td>{{ entity.centro }}</td>
            </tr>
            <tr>
                <th>Ps</th>
                <td>{{ entity.pS }}</td>
            </tr>
            <tr>
                <th>Pcarga</th>
                <td>{{ entity.pCarga }}</td>
            </tr>
            <tr>
                <th>Diskette</th>
                <td>{{ entity.diskette }}</td>
            </tr>
            <tr>
                <th>Clientetiempo</th>
                <td>{{ entity.clienteTiempo|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Emailasociado</th>
                <td>{{ entity.emailAsociado }}</td>
            </tr>
            <tr>
                <th>Tipocliente</th>
                <td>{{ entity.tipoCliente }}</td>
            </tr>
            <tr>
                <th>Cupoasociado</th>
                <td>{{ entity.cupoAsociado }}</td>
            </tr>
            <tr>
                <th>Codigodestinatario</th>
                <td>{{ entity.codigoDestinatario }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cliente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cliente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
